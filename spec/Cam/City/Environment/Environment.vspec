Environment.Temperature_city:
  type: sensor
  datatype: float
  description: Temperature in the city.
  unit: Celsius
  min: -40
  max: 40
  default: 20

Environment.Humidity_city:
  type: sensor
  datatype: float
  description: Humidity in the city.
  unit: Percentage
  min: 0
  max: 100
  default: 50

Environment.Warning:
  type: sensor
  datatype: string
  description: General 

  